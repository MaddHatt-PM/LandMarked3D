{
    "name": "terrain-viewer",
    "version": "0.1.0",
    "author": "Patt Martin",
    "description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
    "homepage": ".",
    "main": "build/electron/main.js",
    "private": true,
    "dependencies": {
        "@babel/core": "^7.0.0-0",
        "@babel/plugin-syntax-flow": "^7.18.6",
        "@babel/plugin-transform-react-jsx": "^7.21.0",
        "@electron/remote": "^2.0.9",
        "@testing-library/dom": "^9.0.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@types/d3": "^7.4.0",
        "@types/electron-devtools-installer": "^2.2.2",
        "@types/jest": "^29.4.0",
        "@types/node": "16.18.12",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-router-dom": "^5.3.3",
        "@types/sharp": "^0.31.1",
        "@types/styled-components": "^5.1.26",
        "d3": "^7.8.2",
        "electron-devtools-installer": "^3.2.0",
        "electron-is-dev": "^2.0.0",
        "electron-reload": "^1.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-is": "^18.2.0",
        "react-router": "^6.9.0",
        "react-router-dom": "^6.9.0",
        "react-scripts": "5.0.1",
        "sharp": "^0.32.0",
        "styled-components": "^5.3.8",
        "typescript": "^4.9.5",
        "web-vitals": "^3.1.1"
    },
    "scripts": {
        "react-start": "react-scripts start",
        "react-build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "coverage": "react-scripts test --env=jsdom --coverage",
        "postinstall": "electron-builder install-app-deps",
        "start": "concurrently \"cross-env BROWSER=none yarn react-start\" \"wait-on http://127.0.0.1:3000 && tsc -p electron -w\" \"wait-on http://127.0.0.1:3000 && tsc -p electron && electron .\"",
        "build": "yarn react-build && tsc -p electron && electron-builder",
        "eject": "react-scripts eject"
    },
    "build": {
        "extends": null,
        "files": [
            "build/**/*"
        ],
        "directories": {
            "buildResources": "assets"
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^7.6.0",
        "cross-env": "^7.0.3",
        "electron": "^23.1.0",
        "electron-builder": "^23.6.0",
        "wait-on": "^7.0.1"
    }
}
